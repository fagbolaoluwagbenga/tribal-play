																	<% } %>	
											
															<% } else if(user.plan.toLowerCase() === "basic"){ %>
											
																	<% if( tvEpisode.category === "free" ){ %>
																	<source src="https://cdn.tribalplay.io/series-video/free/<%= tvEpisode.video_path %>" type="video/mp4">
