																   <source src="https://cdn.tribalplay.io/series-video/basic/<%= tvEpisode.video_path %>" type="video/webm">
																	
																   <% } else if(tvEpisode.category === "premium"){ %>	
																	<source src="https://cdn.tribalplay.io/series-video/premium/<%= tvEpisode.video_path %>" type="video/mp4">
																	<source src="https://cdn.tribalplay.io/series-video/premium/<%= tvEpisode.video_path %>" type="video/webm">
																	<% } %>	
												
															<% } %>
